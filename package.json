{
  "name": "scrap-cerebral-2",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "autoprefixer": "6.5.0",
    "babel-core": "6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-react-app": "^0.2.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "commitizen": "^2.8.6",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.25.0",
    "cz-conventional-changelog": "^1.2.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-react-app": "^0.2.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-flowtype": "^2.18.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.22.0",
    "http-proxy-middleware": "0.17.2",
    "jest": "16.0.1",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "path-exists": "3.0.0",
    "postcss-loader": "0.13.0",
    "promise": "7.1.1",
    "react-addons-test-utils": "^15.3.2",
    "react-dev-utils": "^0.2.1",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "cerebral": "^2.0.0-alpha.4",
    "cerebral-provider-firebase": "^0.1.0",
    "cerebral-router": "^1.0.0-alpha.2",
    "function-tree": "^0.5.0-alpha.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "serve": "npm run build && firebase serve",
    "deploy": "npm run build && firebase deploy"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "description": "Cerebral 2 and [Create React App](https://github.com/facebookincubator/create-react-app).",
  "main": "./src/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saitodisse/scrap-cerebral-2.git"
  },
  "keywords": [
    "cerebral2"
  ],
  "author": "Julio Saito",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/saitodisse/scrap-cerebral-2/issues"
  },
  "homepage": "https://webber-scrapper.firebaseapp.com/",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
