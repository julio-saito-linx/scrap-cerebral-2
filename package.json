    "react-addons-test-utils": "^15.3.2",
    "react-dev-utils": "^0.3.0",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "@kadira/storybook": "2.29.3",
    "cerebral": "next",
    "cerebral-provider-firebase": "next",
    "cerebral-router": "next",
    "concurrently": "^3.1.0",
    "create-react-app": "^0.6.0",
    "firebase": "^3.5.3",
    "firebase-queue": "^1.5.0",
    "function-tree": "^0.5.0-alpha.2",
    "glob": "^7.1.1",
    "lodash": "^4.16.6",
    "nodemon": "^1.11.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "semantic-ui-react": "^0.58.3",
    "tcp-ping": "^0.1.1",
    "winston": "^2.3.0",
    "winston-logstash": "^0.3.0",
    "x-ray": "^2.3.1"
  },
  "scripts": {
    "start": "./scripts/start-all.sh",
    "dev-local-server": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "prod-local-server": "npm run build && firebase serve",
    "deploy": "npm run build && firebase deploy",
    "clean-watchman": "watchman watch-del-all; watchman shutdown-server",
    "storybook": "NODE_ENV=test start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "queue-local-server": "node queue/index.js",
    "start-kibana": "/usr/lib/kibana/bin/kibana",
    "logstash-wait-for-server": "node ./scripts/wait_for_logstash.js",
    "logstash-server": "/opt/logstash/bin/logstash -f queue/utils/logstash_config_elastic.conf"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "description": "Cerebral 2 and [Create React App](https://github.com/facebookincubator/create-react-app).",
  "main": "./src/main.js",
  "repository": {
    "url": "git+https://github.com/saitodisse/scrap-cerebral-2.git",
    "type": "git"
  },
  "keywords": [
    "cerebral2"
  ],
  "author": "Julio Saito",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/saitodisse/scrap-cerebral-2/issues"
  },
  "homepage": "https://webber-scrapper.firebaseapp.com/",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
